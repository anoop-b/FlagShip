		"@tailwindcss/typography": "^0.5.14",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.23.2",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
